                    },
                    "title": "Image Shape",
                    "type": "array"
                },
                "slice_shape": {
                    "description": "Shape of the 2D slice",
                    "items": {
                        "type": "integer"
                    },
                    "title": "Slice Shape",
                    "type": "array"
                },
                "has_mask": {
                    "description": "Whether mask was available and displayed",
                    "title": "Has Mask",
                    "type": "boolean"
                },
                "visualization_path": {
                    "description": "Path to saved visualization PNG",
                    "title": "Visualization Path",
                    "type": "string"
                }
            },
            "required": [
                "subject_id",
                "slice_index",
                "view_plane",
                "image_shape",
                "slice_shape",
                "has_mask",
                "visualization_path"
