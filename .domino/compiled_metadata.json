    "DataSplitPiece": {
        "name": "DataSplitPiece",
        "dependency": {
            "dockerfile": null,
            "requirements_file": "requirements.txt"
        },
        "tags": [
            "data-processing",
            "train-test-split",
            "machine-learning",
            "preprocessing"
        ],
